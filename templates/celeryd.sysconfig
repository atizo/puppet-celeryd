VIRTUALENV="<%= virtualenv %>"
PYHONPATH="<%= pythonpath %>"

# 5 minute hard time limit for tasks + enable events at startup.
CELERYD_OPTS="--time-limit=<%= time_limit %> -E --beat -s /tmp/celerybeat-schedule"

CELERYD_NODES="w1"
CELERYD="<%= celeryd %>"
CELERYD_LOG_FILE=<%= celeryd_log_file %>
CELERYD_PID_FILE=/var/run/celeryd/celeryd.pid

CELERYD_USER="<%= user %>"
CELERYD_GROUP="<%= group %>"
